<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".com.iua.fabio.runningcoloapp.actividades.CoordsEmptyError"
    android:orientation="vertical"
    android:background="@drawable/fondoasd"
    >

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/texto_coords_empty_error"
        android:textColor="#000"
        android:textSize="30dp" />

    <Button
        android:id="@+id/boton_volver_activity_coords_empty_error"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:text="@string/titulo_boton_volver_coords_empty_error" />
</LinearLayout>